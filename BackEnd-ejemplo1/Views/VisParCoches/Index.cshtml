<div class="jumbotron">
    <h1>Vista Parcial</h1>
    <hr />
    <button type="button" id="btnGaraje">Garaje</button>
    <button type="button" id="btnGarajeSimple">GarajeSimple</button>
    <button type="button" id="btnLimpiar">Limpiar Datos</button>
</div>
<div id="divGaraje"></div>
@section Scripts{
    
    <script>
        $(document).ready(function (){
            $("#btnGaraje").click(function () {
                $("#divGaraje").load("_Garaje");
            });
            $("#btnGarajeSimple").click(function () {
                $("#divGaraje").load("_GarajeSimple");
            });
            $("#btnLimpiar").click(function () {
                $("#divGaraje").html("");
            });
        })
    </script>

    }
